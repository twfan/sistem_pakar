<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="e.twfan.sistempakar.ResultActivity">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="5dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Nomor Online"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"/>
                    <View
                        android:id="@+id/view2"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#999999"
                        android:layout_marginTop="5dp"
                        android:layout_below="@+id/txt_header_solusi"
                        android:layout_marginBottom="5dp"/>
                    <TextView
                        android:id="@+id/txt_no_online"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"
                        android:text="12312"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="5dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Jenis Perizinan"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"/>
                    <View
                        android:id="@+id/view3"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#999999"
                        android:layout_marginTop="5dp"
                        android:layout_below="@+id/txt_header_solusi"
                        android:layout_marginBottom="5dp"/>
                    <TextView
                        android:id="@+id/txt_jenis_izin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"
                        android:text="SIUP"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="5dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Bentuk Usaha"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"/>
                    <View
                        android:id="@+id/view5"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#999999"
                        android:layout_marginTop="5dp"
                        android:layout_below="@+id/txt_header_solusi"
                        android:layout_marginBottom="5dp"/>
                    <TextView
                        android:id="@+id/txt_bentuk_usaha"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"
                        android:text="Perorangan"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="5dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Perusahaan"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"/>

                    <View
                        android:id="@+id/view6"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/txt_header_solusi"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:background="#999999" />
                    <TextView
                        android:id="@+id/txt_nama_perusahaan"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"
                        android:text="PT.ABC"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="5dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Status Berkas"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"/>

                    <View
                        android:id="@+id/view7"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/txt_header_solusi"
                        android:textAlignment="center"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:background="#999999" />
                    <TextView
                        android:id="@+id/txt_posisi_berkas"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"
                        android:text="KABAG"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="5dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Posisi Berkas"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"/>

                    <View
                        android:id="@+id/view8"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/txt_header_solusi"
                        android:textAlignment="center"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:background="#999999" />
                    <TextView
                        android:id="@+id/txt_status_berkas"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textSize="@dimen/text_size_result_activity"
                        android:text="PROSES VERIFIKASI"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/txt_header_solusi"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Solusi"
                        android:textSize="@dimen/text_size_result_activity"
                        android:textAlignment="center" />
                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#999999"
                        android:layout_marginTop="5dp"
                        android:layout_below="@+id/txt_header_solusi"
                        android:layout_marginBottom="5dp"/>

                    <TextView
                        android:id="@+id/txt_solusi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="hsalkfjhskajhsadkfjhsdkfjhskadjfhalskjdhfklsajhdfkjsahdfkajshdfkjashfkjhaskdfjhaksjdfhkasjdhfakjshdfkjashdfkjshdfakjshdflkjahsdfkjashdfkjh"
                        android:textAlignment="center"
                        />
                </LinearLayout>
            </android.support.v7.widget.CardView>

        </LinearLayout>



    </ScrollView>

</LinearLayout>
